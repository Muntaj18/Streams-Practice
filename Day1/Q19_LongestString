import java.util.stream.*;
import java.util.*;
class LongestString{
    public static void main(String[] args) {
        
     List<String> list = List.of("cat","tiger","lion","elephant");
     
    String longest= list.stream()
     .max(Comparator.comparingInt(String::length))
     .orElse(null);
     System.out.println(longest);
    }
}
