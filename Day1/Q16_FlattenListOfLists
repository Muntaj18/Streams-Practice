import java.util.stream.*;
import java.util.*;
class FlattenListOfLists {
    public static void main(String[] args) {
        
       List<List<Integer>> list=List.of(
           
           List.of(1,2),
           List.of(3,4),
           List.of(6,7)
           );
         List<Integer> result=
         list.stream()
         .flatMap(Collection::stream)
         .toList();
        System.out.println(result);
    }
}
