import java.util.stream.*;
import java.util.*;
class AverageOfNumbers {
    public static void main(String[] args) {
        
     List<Integer> list = List.of(10,5,8,20,15);
     
     double avg=
    list.stream()
    .collect(Collectors.averagingInt(Integer::intValue));
    
    System.out.println(avg);
    
    
    }
}
