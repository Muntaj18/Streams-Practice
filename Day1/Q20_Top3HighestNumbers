import java.util.stream.*;
import java.util.*;
class Top3HighestNumbers{
    public static void main(String[] args) {
        
     List<Integer> list = List.of(10,5,20,15,30,25);
     
   List<Integer> result=
   list.stream()
   .sorted(Comparator.reverseOrder())
   .limit(3)
   .toList();
   System.out.println(result);
    }
}
