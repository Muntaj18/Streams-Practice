import java.util.*;
import java.util.stream.*;
class remvDuplicatesPrintOnlyUnique {
    public static void main(String[] args) {
        
       List<Integer> list=List.of(1,2,3,1,2,4,3,5);
       
      Map<Integer, Long> freq=
      list.stream()
      .collect(Collectors.groupingBy(n->n,Collectors.counting()));
      
      List<Integer> result=
      list.stream()
      .filter(n->freq.get(n)==1)
      .toList();
       System.out.println(result);
    }
}
