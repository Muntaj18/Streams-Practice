
import java.util.*;
import java.util.stream.*;
class SecondHighestWithFreq {
    public static void main(String[] args) {
        
       List<Integer> list=List.of(1,20,30,50,45);
       
       Integer result=
       list.stream()
       .sorted(Comparator.reverseOrder())
       .skip(1)
       .findFirst()
       .orElse(null);
       
       Long freq=
      list.stream()
      .filter(n->n.equals(result))
      .count();
       
       
       System.out.println( " second highest number:"+result + " with frequency:" +freq);
    }
}
